{
  "name": "active-touch",
  "version": "1.3.3",
  "description": "A solution for :active CSS problem on iOS devices",
  "main": "build/bundle.js",
  "scripts": {
    "prerelease": "babel --presets es2015 src/index.js -o build/bundle.js",
    "test": "babel --presets es2015 src/index.js src/test.js -o build/bundle.js && http-server",
    "start": "http-server"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es2015-modules-umd"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmitrizzle/active-touch.git"
  },
  "keywords": [
    "touch",
    "interaction",
    "styles",
    "javascript"
  ],
  "author": "dmitrizzle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmitrizzle/active-touch/issues"
  },
  "homepage": "https://github.com/dmitrizzle/active-touch#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "http-server": "^0.9.0"
  }
}
